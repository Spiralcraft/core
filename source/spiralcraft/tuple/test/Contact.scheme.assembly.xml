<!-- Assembly based Scheme for a personal Contact -->
<tuple:Scheme
  xmlns:tuple="java:/spiralcraft/tuple"
  >
  <fields>
    <tuple:Field>
      <name>firstName</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>lastName</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>phone</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>address</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>city</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>state</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
    <tuple:Field>
      <name>zip</name>
      <type>
        <tuple:Type>
          <javaClass>java.lang.String</javaClass>
        </tuple:Type>
      </type>
    </tuple:Field>
  </fields>
</tuple:Scheme>
