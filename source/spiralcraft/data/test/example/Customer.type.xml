<?xml version="1.0" encoding="UTF-8"?>

<meta:Type
  xmlns:meta="java:/spiralcraft/data/types/meta/"
  xmlns:example="java:/spiralcraft/data/test/example"
  >
  <archetype><example:Contact.type/>
  </archetype>
  
  <scheme>
    <meta:Scheme>

      <fields>
        <meta:Field>

          <name>id
          </name>

          <type><Integer.type/>
          </type>
          
        </meta:Field>
        
        <meta:Field>
        
        	<name>active
        	</name>
        	
        	<type><Boolean.type/>
        	</type>
        	
        </meta:Field>
        
        <meta:Field>

          <name>paymentTerms
          </name>

          <type>
          	<String.type>
          	
              <maxLength>32
              </maxLength>
              
          	</String.type>
          </type>

        </meta:Field>
      </fields>
      
      <keys>
      	<meta:Key>

      		<fieldList>id
      		</fieldList>
      		
      		<primary>true
      		</primary>
      		
      	</meta:Key>
      </keys>
      
    </meta:Scheme>
  </scheme>
  

</meta:Type>