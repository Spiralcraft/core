<meta:Type
  xmlns:meta="class:/spiralcraft/data/types/meta/"
  xmlns:example="class:/spiralcraft/data/test/example/"
  >
  <fields>
    <meta:SequenceField>
      <name>id
      </name>
      <type><LSID.type/>
      </type>
    </meta:SequenceField>
    <meta:Field>
      <name>city
      </name>
      <type><String.type/>
      </type>
    </meta:Field>
    <meta:Field>
      <name>state
      </name>
      <type><String.type/>
      </type>
    </meta:Field>
  </fields>
  <keys>
    <meta:Key>
      <primary>true
      </primary>
      <fieldList>id
      </fieldList>
    </meta:Key>
    <meta:Key>
      <unique>true
      </unique>
      <fieldList>city,state
      </fieldList>
    </meta:Key>
  </keys>
  <methods>
    <meta:TaskMethod>
      <name>normalize
      </name>
      <static>true
      </static>
      <parameterTypes>
        <String.type/>
        <String.type/>
      </parameterTypes>
      <returnType>
        <example:CityState.type/>
      </returnType>
    </meta:TaskMethod>  
  </methods>
</meta:Type>